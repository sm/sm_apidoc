#!/bin/sh

set -v # Turn on verbose logging, so that the following will present a tutorial.

# file(s) module api is loaded with the line 'api/files' in shell/includes.

# We can test if all files given are executable and act according to the result:
if files are executable /bin/zsh /bin/bash
then
  log "/bin/zsh and /bin/bash are executable :)"
else
  log "/bin/zsh and/or bin/bash are not executable :("
fi

